<Window x:Class="LAP_2018_1_Silosteuerung.SetManual.SetManualWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LAP_2018_1_Silosteuerung.SetManual"
        mc:Ignorable="d"
        Title="SetManualWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="10" />
            <ColumnDefinition Width="10" />
            <ColumnDefinition Width="70" />
            <ColumnDefinition Width="70" />
            <ColumnDefinition Width="70" />
            <ColumnDefinition Width="70" />
            <ColumnDefinition Width="70" />
            <ColumnDefinition Width="70" />
            <ColumnDefinition Width="70" />
            <ColumnDefinition Width="70" />
            <ColumnDefinition Width="70" />
            <ColumnDefinition />
            <ColumnDefinition Width="10" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="10" />
            <RowDefinition Height="10" />
            <RowDefinition Height="35" />
            <RowDefinition Height="35" />
            <RowDefinition Height="35" />
            <RowDefinition Height="35" />
            <RowDefinition Height="35" />
            <RowDefinition Height="35" />
            <RowDefinition Height="35" />
            <RowDefinition Height="35" />
            <RowDefinition Height="35" />
            <RowDefinition Height="35" />
            <RowDefinition Height="*" />
            <RowDefinition Height="10" />
        </Grid.RowDefinitions>
        <Button Grid.Column="3" Grid.Row="3" Content="Q1"       ClickMode="{Binding ViAnzeige.ClickModeBtnQ1}"      Command="{Binding BtnQ1_LL}"/>
        <Button Grid.Column="3" Grid.Row="5" Content="Q2"       ClickMode="{Binding ViAnzeige.ClickModeBtnQ2}"      Command="{Binding BtnQ2}"/>
        <Button Grid.Column="3" Grid.Row="7" Content="K1"       ClickMode="{Binding ViAnzeige.ClickModeBtnK1}"      Command="{Binding BtnK1}"/>
        <Button Grid.Column="5" Grid.Row="6" Content="Q1 + K1"  ClickMode="{Binding ViAnzeige.ClickModeBtnQ1_K1}"   Command="{Binding BtnQ1_K1}" Grid.ColumnSpan="2" Grid.RowSpan="2"/>
    </Grid>
</Window>
